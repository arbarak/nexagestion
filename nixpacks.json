{
  "providers": ["node"],
  "variables": {
    "NODE_ENV": "production",
    "NEXT_TELEMETRY_DISABLED": "1"
  },
  "phases": {
    "setup": {
      "cmds": [
        "mkdir -p /app/.next/tsbuildinfo"
      ]
    },
    "install": {
      "dependencyFile": "package-lock.json",
      "dependencyFileCommand": "npm ci"
    },
    "build": {
      "cmds": [
        "npm run build"
      ]
    },
    "start": {
      "run": "npm run start"
    }
  }
}
